#!/bin/bash

echo "RLLibVizContinuousGridworld"

qmake RLLibVizContinuousGridworld.pro

make $1  

make clean

echo "RLLibVizMountainCar"

qmake RLLibVizMountainCar.pro

make $1

make clean

echo "RLLibVizSwingPendulum"

qmake RLLibVizSwingPendulum.pro

make  $1

make clean

qmake RLLibVizSwingPendulum2.pro

make  $1

make clean

rm Makefile
